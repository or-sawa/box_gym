<%= form_with(model: @event, local: true) do |form| %>

  <div class="time">
    <div class="form-tag">開始時間</div>
    <%= form.datetime_select :started_at %>
  </div>

  <div class="time">
    <div class="form-tag">終了時間</div>
    <%= form.datetime_select :finished_at %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>
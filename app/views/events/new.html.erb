<% provide(:title) %>
<%= form_with(model: @event, local: true) do |form| %>

  <div class="time">
    <%= form.label :started_at %>
    <%= form.datetime_select :started_at %>
  </div>

  <div class="time">
    <%= form.label :finished_at %>
    <%= form.datetime_select :finished_at %>
  </div>


  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>